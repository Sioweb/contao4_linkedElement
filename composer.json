{
  "name": "sioweb/linkedelement",
  "type": "contao-bundle",
  "description": "LinkedElement for Contao 4 & 5",
  "license": "MIT",
  "authors": [
    {
      "name": "Sascha Weidner",
      "email": "support@sioweb.de"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^4.13 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "Sioweb\\LinkedElementBundle\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/config/",
      "contao/dca/",
      "contao/languages/",
      "contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Sioweb\\LinkedElementBundle\\ContaoManager\\Plugin"
  }
}
